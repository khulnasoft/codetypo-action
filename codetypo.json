{
    "version": "0.2",
    "$schema": "https://raw.githubusercontent.com/khulnasoft/codetypo/main/codetypo.schema.json",
    "enableGlobDot": true,
    "words": [
        "camelcase",
        "codecov",
        "cosmiconfig",
        "coverallsapp",
        "EDITMSG",
        "lcov",
        "octokit",
        "openapi",
        "pollyjs",
        "repos",
        "shrinkwrap",
        "khulnasoft"
    ],
    "useGitignore": true,
    "ignorePaths": [
        ".git/*",
        ".git/!{COMMIT_EDITMSG,EDITMSG}",
        ".git/*/**",
        ".yarn",
        ".pnp.js",
        ".vscode",
        ".gitignore",
        "action/lib/**",
        "coverage",
        "codetypo.json",
        "__snapshots__",
        "__recordings__",
        "**/coverage/**",
        "**/fixtures/**/*.json",
        "**/fixtures/sampleCode/*errors/",
        "node_modules",
        "main_root.js",
        "package-lock.json",
        "pnpm-lock.yaml",
        "yarn.lock"
    ],
    "patterns": [
        {
            "name": "Email",
            "pattern": "/<?\\b[\\w.\\-+]{1,64}@\\w{1,63}(\\.\\w{1,63}){1,3}\\b>?/gi"
        }
    ],
    "overrides": [
        {
            "filename": "*.har",
            "languageId": "json",
            "dictionaries": [
                "http",
                "node",
                "typescript"
            ],
            "words": [
                "etag",
                "maxage",
                "ratelimit",
                "nosniff"
            ]
        }
    ],
    "enableFiletypes": [
        "shellscript"
    ]
}
